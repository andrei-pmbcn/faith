{
  "name": "faith-of-the-apostles",
  "version": "0.0.1",
  "description": "A Christian HTML5 game with unique, non-violent but combat-like social encounter mechanics. This package provides a demo of these social encounter mechanics.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config webpack/base.js",
    "build": "webpack --config webpack/prod.js",
    "builddev": "webpack --config webpack/base.js",
    "test": "webpack -w --config webpack/test.js",
    "doc": "jsdoc src --recurse --private --template ./node_modules/ink-docstrap/template --destination ./doc"
  },
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-root-import": "^6.6.0",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.7",
    "mocha": "^9.0.1",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "Christian",
    "Christianity",
    "game",
    "RPG",
    "strategy"
  ],
  "author": "Andrei Pambuccian",
  "license": "GPL-3.0-or-later"
}
